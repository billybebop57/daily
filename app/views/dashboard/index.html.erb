<div class="container">

<h1>All Players</h1>

	<div class="pure-g">
	    <div class="pure-u-1-5">
	    	<h2>Top 5 Projected</h2>
			<% @all_players.top_projected.each do |all| %>
				<%= "#{all[:name]}\t(#{all[:avg_proj].to_f.round(1)} pts)" %><br>
			<% end %>
	    </div>
	    <div class="pure-u-1-5">
	    	<h2>Top 5 Value</h2>
			<% @all_players.top_value.each do |all| %>
				<%= "#{all[:name]}\t(#{all[:avg_proj].to_f.round(1)} pts)" %><br>
			<% end %>
	    </div>
	</div>

</div>





